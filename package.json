{
  "name": "mal-calendar",
  "version": "0.0.0",
  "description": "Chrome extension to track and display on a weekly calendar the animes that a MAL user has in the currently watching list.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run clean && webpack --config webpack/webpack.dev.js",
    "build": "npm run clean && webpack --config webpack/webpack.prod.js"
  },
  "keywords": [
    "Extension",
    "Chrome",
    "MAL",
    "MyAnimeList",
    "Anime"
  ],
  "author": "Pablo Silio Polanco",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.133",
    "@types/chrome": "^0.0.176",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "copy-webpack-plugin": "^10.2.1",
    "css-loader": "^6.5.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
